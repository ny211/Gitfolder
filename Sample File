# def addition(a, b):

#   summation = a + b

#   return summation


def subtraction(a, b):

  difference = a - b

  return difference


def division(a, b):

  quotient = a / b

  return quotient


def nice_multiplication(a, b):

  product = a * 2b

  return product


def nice_division(a, b):

  quotient = (a+13) / b

  return quotient