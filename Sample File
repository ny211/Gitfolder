def subtraction(a, b):

  difference = a - b

  return difference


def division(a, b):

  quotient = a / b

  return quotient


def not_so_nice_multiplication(a, b):
def nice_multiplication(a, b):

  product = a * 2b

  return product


def really_bad_division(a, b):
def nice_division(a, b):

  quotient = (a+13) / b

  return quotient