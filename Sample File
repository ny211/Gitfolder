# def addition(a, b):

#   summation = a + b

#   return summation


def subtraction(a, b):

  difference = a - b

  return difference


def division(a, b):

  quotient = a / b

  return quotient


def multiplication(a, b):

  product = a * b

  return product


def bad_division(a, b):

  quotient = (a+13) / b

  return quotient